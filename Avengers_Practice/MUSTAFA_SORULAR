    public Animal(String name, String breed, char gender, int age, char size, String color) {
        this.name = name;
        this.breed = breed;
        this.gender = gender;
        this.age = age;
        this.size = size;
        this.color = color;
    }

    public void setInfo(String name, String breed, char gender, int age, char size, String color) {
